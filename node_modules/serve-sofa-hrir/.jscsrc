{
  "preset": "airbnb",
  "disallowSpacesInsideArrayBrackets": false,
  "disallowSpacesInsideBrackets": false,
  "disallowSpacesInsideParentheses": false,
  "disallowSpacesInsideTemplateStringPlaceholders": false,
  "requireCurlyBraces": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "try",
    "catch"
  ],
  "requirePaddingNewLinesAfterBlocks": false,
  "requirePaddingNewLinesBeforeLineComments": false,
  "requireShorthandArrowFunctions": false,
  "requireSpacesInsideObjectBrackets": false,
  "plugins": [
    "jscs-jsdoc"
  ],
  "jsDoc": {
    "checkAnnotations": true,
    "checkParamNames": true,
    "checkRedundantParams": true,
    "checkRedundantReturns": true,
    "checkReturnTypes": true,
    "checkTypes": true,
    "enforceExistence": {
      "allExcept": [
        "arrow",
        "expressions"
      ]
    },
    "requireParamTypes": true,
    "requireReturnTypes": true
  }
}
